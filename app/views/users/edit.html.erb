<h1>ポケモントレーナー編集画面</h1>

<%= form_with model: @user do |f| %>
  <%= f.label :name, "名前" %>
  <%= f.text_field :name %>

  <%= f.label :age, "年齢" %>
  <%= f.number_field :age %>

  <%= f.label :address, "住所" %>
  <%= f.text_field :address%>

  <%= f.submit %>
<% end %>

<%= link_to "トップ画面", users_path %>