<div class="field">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select only: [:name, :age, :address], associations: [:pokemons] %>
  <% end %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>
  <% end %>
  <%= f.predicate_select only: [:eq, :lteq, :gteq, :start, :end, :cont, :not_cont], compounds: false %>
  <%= link_to "削除", "#", class: "remove_fields" %>
</div>